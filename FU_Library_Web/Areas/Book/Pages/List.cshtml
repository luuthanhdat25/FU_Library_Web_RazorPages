            max-width: 100%;
            height: auto;
        }

    .book-detail {
        flex: 1;
        display: flex;
        flex-direction: column;
    }

        .book-detail a.title {
            font-size: 16px;
            font-weight: bold;
            color: #0066cc;
            text-decoration: none;
            margin-bottom: 10px;
        }

            .book-detail a.title:hover {
                text-decoration: underline;
            }

        .book-detail p {
            font-size: 14px;
            color: #333;
            margin: 5px 0;
        }

            .book-detail p strong {
                color: #333;
            }

    /* Styling for clearing floats */
    .clear {
        clear: both;
    }
</style>
